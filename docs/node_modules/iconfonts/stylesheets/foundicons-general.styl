fig = {
  path: "https://cdn.rawgit.com/zurb/foundation-icons/master/foundation_icons_general/fonts/"
  name: "GeneralFoundicons"
  site: "http://zurb.com/playground/foundation-icon-fonts-3"
  basename: "general_foundicons"
  version: "1.5.2"
  svghash: "#GeneralFoundicons"
  nativesize: "32px"
}

fig-font()
  @font-face
    font-family: fig['name']
    src: url(fig['path'] + fig['basename'] + '.eot?v=' + fig['version'])
    src: url(fig['path'] + fig['basename'] + '.eot?#iefix&v=' + fig['version']) format("embedded-opentype"),
         url(fig['path'] + fig['basename'] + ".woff?v=" + fig['version']) format("woff"),
         url(fig['path'] + fig['basename'] + ".ttf?v=" + fig['version']) format("truetype"),
         url(fig['path'] + fig['basename'] + ".svg?v=" + fig['version'] + fig['svghash']) format("svg")
    font-weight: normal
    font-style: normal

//
// Uses a given icon.
//
//     .button:before
//       fig-icon("music")
//       fig-icon("music", 24px)        /* 24px size */
//
// You may specify a color.
//
//     .button:before
//       fig-icon("music", color: #333)
//
// You may also specify a `top` value to compensate for any mis-alignment.
// This nudges the icon by that many pixels from the top.
//
//     .button:before
//       fig-icon("music", top: 2px)

fig-icon(type, size = auto, color = auto, top = auto, left = auto, shadow = none)
  @extend $fig-icon
  content: fig-icons[type]

  unless size is "auto"
    font-size: size
  unless top is "auto"
    position: relative
    top: top
  unless left is "auto"
    position: relative
    left: left
  unless color is "auto"
    color: color
  unless shadow is "none"
    text-shadow: shadow

$fig-icon
  line-height: 1em
  font-family: fig['name']
  font-weight: normal
  font-style: normal
  display: inline-block
  text-decoration: none
  vertical-align: middle
  text-rendering: optimizeLegibility !important
  -webkit-font-smoothing: antialiased !important
  -moz-osx-font-smoothing: grayscale

